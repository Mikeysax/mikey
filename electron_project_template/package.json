{
  "name": "MikeyGeneratedElectronApp",
  "version": "0.0.1",
  "description": "Mikey Generated Electron application",
  "main": "main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "dev": "cross-env NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development & npm run dev-server",
    "dev-server": "webpack-dev-server --progress --color",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "test": "NODE_ENV=production mocha './tests/**/*.test.@(js|jsx)' --compilers js:babel-core/register --require ./tests/testHelpers.js 'tests/**/*.@(js|jsx)' --require ignore-styles",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Electron"
  ],
  "author": "Michael Farrell",
  "license": "MIT",
  "homepage": "",
  "bugs": {
    "url": ""
  },

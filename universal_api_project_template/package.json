{
  "name": "UniversalAPIMikeyGeneratedApp",
  "version": "0.0.1",
  "description": "An Universal/Isomorphic application with API using WebPack, ReactJS, Redux, and ES6.",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony .",
    "dev": "NODE_ENV=development npm run start & webpack-dev-server --quiet --progress --color",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js",
    "test": "mocha './tests/**/*.test.@(js|jsx)' --compilers js:babel-core/register --require ./tests/testHelpers.js 'tests/**/*.@(js|jsx)' --require ignore-styles",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src",
    "heroku-prebuild": "rm yarn.lock",
    "heroku-postbuild": "npm run build",
    "seq": "node_modules/.bin/sequelize",
    "migrate": "node_modules/.bin/sequelize db:migrate"
  },
  "repository": {
    "type": "git",

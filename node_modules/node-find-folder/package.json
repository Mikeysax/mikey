{
  "name": "node-find-folder",
  "description": "Walk a directory tree in node.js. Use just the name to find the folder(s) you want, rather than through given path(s).",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/gulp",
    "watch": "cross-env NODE_ENV=development ./node_modules/.bin/gulp --nopreprocess",
    "precommit": "cross-env NODE_ENV=development ./node_modules/.bin/gulp --precommit",
    "test": "cross-env NODE_ENV=development ./node_modules/.bin/mocha",
    "updated-authors": "sh ./tool/authors.sh"
  },
  "dependencies": {
    "glob": "^7.1.2",
    "is-directory": "^0.3.1",
    "is-file": "^1.0.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "coffee-script": "^1.12.6",
    "cross-env": "^5.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.1.0",
    "gulp-coffee": "^2.3.4",
    "gulp-coffeelint": "^0.6.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "lazypipe": "^1.0.1",
    "merge-stream": "^1.0.1",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2"
  },
  "keywords": [
    "node-find-folder",
    "gulp-find-folder",
    "grunt-find-folder",
    "find just the folders",
    "match folders",
    "search folders",
    "traversal folders",
    "find directory",
    "search directory",
    "traversal directory",
    "walk a directory tree",
    "scan a directory tree",
    "search a directory tree",
    "get paths of specific folders",
    "scan dir",
    "scan directory",
